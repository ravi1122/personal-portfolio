{
  "name": "personal-portfolio",
  "version": "1.0.0",
  "private": true,
  "homepage": "https://ravi1122.github.io/personal-portfolio",
  "dependencies": {
    "@emailjs/browser": "^4.4.1",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^13.5.0",
    "framer-motion": "^10.16.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.11.0",
    "react-scripts": "5.0.1",
    "react-type-animation": "^3.2.0",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "build:analyze": "npm run build && npx webpack-bundle-analyzer build/static/js/*.js",
    "build:optimized": "GENERATE_SOURCEMAP=false npm run build",
    "test": "react-scripts test",
    "test:coverage": "react-scripts test --coverage --no-watch",
    "eject": "react-scripts eject",
    "performance:audit": "lighthouse http://localhost:3000 --only-categories=performance --chrome-flags=\"--headless\"",
    "performance:monitor": "node -e \"console.log('Check browser dev tools for performance monitoring data')\"",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "deploy:netlify": "npm run build && npx netlify deploy --prod --dir=build",
    "deploy:vercel": "npm run build && npx vercel --prod"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "autoprefixer": "^10.4.14",
    "gh-pages": "^6.3.0",
    "postcss": "^8.4.24",
    "tailwindcss": "^3.3.2"
  }
}
